package xjtu.citi.entity;

import java.util.HashSet;
import java.util.Set;
public class UserType {

	private int id;
	private String name;    //用户类型名称
	
	private Set users = new HashSet();
	
	public UserType(int id, String name, Set users) {
		super();
		this.id = id;
		this.name = name;
		this.users = users;
	}
	

	public UserType(int id) {
		super();
		this.id = id;
	}


	public UserType() {
		super();
	}


	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Set getUsers() {
		return users;
	}

	public void setUsers(Set users) {
		this.users = users;
	}
	
	
}
