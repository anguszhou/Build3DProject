<?xml version="1.0" encoding="gbk"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping  package="xjtu.citi.entity" >

	<class name="TransInfo"  table="transinfo"  >
		<id name="id" column="id"  >
			<generator class="native"/>
		</id>
        <property name="accountIn"    column="accountIn" ></property>
        <property name="accountOut"   column="accountOut" ></property>
        <property name="amount"   column="amount" ></property>
        <property name="time"   column="time" ></property>
           
   
    </class>
</hibernate-mapping>