<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.edu.xjtu.ects.entity">
    <class name="notice" table="notice">
    	<id name="id" column="id" unsaved-value="null" >
			<generator class="native"/>
		</id>
        <property name="noticeTitle"                 column="noticeTitle" ></property>
        <property name="noticeContext"               column="noticeContext" ></property>
         <property name="noticeDate"               column="noticeDate" ></property>
       
    </class>
</hibernate-mapping>