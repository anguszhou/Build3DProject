<?xml version="1.0" encoding="gbk"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping  package="cn.edu.xjtu.ects.entity" >

	<class name="Order"  table="myorder"  >
		<id name="order_id" column="order_id" unsaved-value="null" >
			<generator class="native"/>
		</id>
		 
        <property name="name"  column="name" ></property>
        <property name="cost"  column="cost" ></property>
       
     <many-to-one name="user" class="User"  column="user_id" fetch = "join"/>  
      <many-to-one name="orderStatus" class="OrderStatus" column="orderStatus_id" fetch="join"/>  
     
          <set name="orderLines"      fetch="join"  > 
			<key column="order_id"/>
			<one-to-many class="OrderLine"/> 
        </set> 
        
    </class>
    
 
   
</hibernate-mapping>

 


