<?xml version="1.0" encoding="gbk"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping  package="cn.edu.xjtu.ects.entity" >

  <class name="OrderStatus"  table="orderstatus"  >
		<id name="orderStatus_id" column="orderStatus_id" unsaved-value="null" >
			<generator class="native"/>
		</id>
        <property name="name"                 column="name" ></property>
        <property name="descriptions"                 column="descriptions" ></property>
        
    </class>
   
</hibernate-mapping>

 


